func main() {
    // arrays
    let a: []int = [];
    a[0] = 0;
    a[1] = 10;
    a[2] = 20;
    a[30] = 30;
    assert(a[0] == 0);
    assert(a[1] == 10);
    assert(a[2] == 20);
    assert(a[30] == 30);

    // assignments with arrays
    let g: []int = [];
    g[10] = 10;
    let h = g[10];
    assert(g[10] == 10);
    assert(h == 10);
    assert(g[10] == h);
    g[10] = 12;
    h = g[10];
    assert(g[10] == 12);
    assert(h == 12);
    assert(g[10] == h);
}